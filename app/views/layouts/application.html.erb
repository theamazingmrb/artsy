<!DOCTYPE html>
<html>
  <head>
    <title>AuthenticationApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <h1>Welcome Home <%= current_user.user_name if current_user%> | <%= link_to 'Log In', login_path %> | <%= link_to 'Log Out', logout_path, method: :delete %></h1>
    <%= yield %>
  </body>
</html>
